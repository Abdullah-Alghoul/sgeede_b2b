function CheckPassword(new_password,confirm_pwd) {
    if(new_password== confirm_pwd)
    {
        password_confirm = "<div class='match'>Passwords match!</div>";
        $(".password_confirm").html(password_confirm)

    }else{
        password_confirm = "<div class='not-match'>Passwords do not match!</div>";
        $(".password_confirm").html(password_confirm)	
    }
}

  
